{
  "name": "lyenrowe/omnipay-unionpay",
  "type": "library",
  "description": "UnionPay gateway for Omnipay payment processing library",
  "keywords": [
    "union",
    "unionpay",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "purchase",
    "银联",
    "sdk",
    "无跳转支付",
    "laravel"
  ],
  "homepage": "https://github.com/lyenrowe/omnipay-unionpay",
  "license": "MIT",
  "authors": [
    {
      "name": "Loki Else",
      "email": "lyen.rowe@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnipay\\UnionPay\\": "src/"
    },
    "files": [
      "src/Common/helpers.php"
    ]
  },
  "require": {
    "omnipay/common": "v3.0-alpha.1",
    "php-http/guzzle6-adapter": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "omnipay/tests": "^3.0",
    "squizlabs/php_codesniffer": "3.*",
    "http-interop/http-factory-guzzle": "^1.2",
    "php-http/message-factory": "1.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
